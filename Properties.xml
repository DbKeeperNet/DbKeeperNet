<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Condition="exists('UserProperties.xml')" Project="UserProperties.xml" />
  <PropertyGroup>
    <Configuration Condition="'$(Configuration)' == ''">Release</Configuration>
    <GitTool Condition="'$(GitTool)' == ''">git</GitTool>
    <GitRepository Condition="'$(GitRepository)' == ''">https://github.com/voloda/DbKeeperNet.git</GitRepository>
    <TmpDir Condition="'$(TmpDir)' == ''">$(TMP)</TmpDir>
    <DestinationDir Condition="'$(DestinationDir)' == ''">.</DestinationDir>
    <SrcRootDir Condition="'$(SrcRootDir)' == ''">.</SrcRootDir>
    <NUnitTool Condition="'$(NUnitTool)' == ''">nunit-console.exe</NUnitTool>
    <XsdTool Condition="'$(XsdTool)' == ''" >xsd.exe</XsdTool>
    <NDocTool Condition="'$(NDocTool)' == ''" >ndoc3console.exe</NDocTool>
    <MSBuildCommunityTasksPath>$(MSBuildProjectDirectory)\.build\</MSBuildCommunityTasksPath>
  </PropertyGroup>
</Project>
