<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="dbkeeper.net" type="DbKeeperNet.Engine.DbKeeperNetConfigurationSection,DbKeeperNet.Engine"/>
  </configSections>
  <dbkeeper.net loggingService="fx">
    <databaseServiceMappings>
      <add connectString="mock" databaseService="MockDriver" />
      <add connectString="mssql" databaseService="MsSql" />
    </databaseServiceMappings>
    <extensions>
      <!-- <add assemblyPath="" /> -->
    </extensions>
  </dbkeeper.net>
  <connectionStrings>
    <add name="mssql"
      connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\DbKeeperNetNUnit.mdf;Integrated Security=True;User Instance=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>